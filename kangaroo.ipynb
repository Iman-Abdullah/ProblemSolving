      "source": [
        "def kangaroo(x1, v1, x2, v2):\n",
        "    if v1 == v2:\n",
        "        return \"YES\" if x1 == x2 else \"NO\"\n",
        "    n = (x2 - x1) / (v1 - v2)\n",
        "    return \"YES\" if n.is_integer() and n >= 0 else \"NO\"\n",
        "\n",
        "\n",
        "print(kangaroo(2, 1, 1, 2))\n",
        "print(kangaroo(0, 3, 4, 2))\n",
        "print(kangaroo(0, 2, 5, 3))\n"
      ]
    }
  ]
